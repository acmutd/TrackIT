e":"src/Components/Pages/Pricing.js","original":{"line":72,"column":10}},{"generated":{"line":61,"column":454},"source":"src/Components/Pages/Pricing.js","original":{"line":71,"column":8}},{"generated":{"line":61,"column":455},"source":"src/Components/Pages/Pricing.js","original":{"line":69,"column":6}},{"generated":{"line":61,"column":456},"source":"src/Components/Pages/Pricing.js","original":{"line":68,"column":4}},{"generated":{"line":62,"column":0},"source":"src/Components/Pages/Pricing.js","original":{"line":88,"column":3}},{"generated":{"line":64,"column":0},"source":"src/Components/Pages/Pricing.js","original":{"line":15,"column":38}},{"name":"Pricing","generated":{"line":66,"column":15},"source":"src/Components/Pages/Pricing.js","original":{"line":91,"column":15}}],"sources":{"src/Components/Pages/Pricing.js":"import * as React from 'react';\nimport Navbar from \"../Layout/NavBar\";\nimport {\n  Button,\n  Container,\n  Card,\n  CardDeck,\n  ListGroup,\n  ListGroupItem,\n} from \"react-bootstrap\";\n\n/**\n * Displayes a grid of pricing options for the product\n */\nclass Pricing extends React.Component {\n  constructor(props) {\n    super(props);\n    this.state = {\n      tiers: [\n        {\n          name: \"Free\",\n          price: \"$0/mo\",\n          perks: [\n            \"Up to 5 Workshop Levels\",\n            \"Limited to 20 students\",\n            \"No concurrent workshops\",\n          ],\n        },\n        {\n          name: \"Pro\",\n          price: \"$10/mo\",\n          perks: [\n            \"Up to 25 Workshop Levels\",\n            \"Limited to 100 students\",\n            \"5 concurrent workshops\",\n          ],\n        },\n        {\n          name: \"Enterprise\",\n          price: \"$50/mo\",\n          perks: [\n            \"Unlimited Workshop Levels\",\n            \"Unlimited students\",\n            \"Unlimited concurrent workshops\",\n          ],\n        },\n      ],\n    };\n  }\n\n  render() {\n    let tierCards = this.state.tiers.map((item, index) => (\n      <Card key={index} style={{ width: \"18rem\" }} className=\"floating-icon\">\n        <Card.Img variant=\"top\" />\n        <Card.Body>\n          <Card.Title>{item.name}</Card.Title>\n          <Card.Text>{item.price}</Card.Text>\n        </Card.Body>\n        <ListGroup className=\"list-group-flush\">\n          <ListGroupItem>{item.perks[0]}</ListGroupItem>\n          <ListGroupItem>{item.perks[1]}</ListGroupItem>\n          <ListGroupItem>{item.perks[2]}</ListGroupItem>\n        </ListGroup>\n        <Button>Sign Up!</Button>\n      </Card>\n    ));\n\n    return (\n      <div>\n        <Navbar />\n        <Container fluid>\n          <div className=\"m-5 p-5 floating-icon\">\n            <h1>Pricing</h1>\n            <p className=\"mt-3\">\n              We try our best to offer our services for free for personal use as\n              one of our core beliefs is free education. However, nothing is\n              ever truly free. We still need to host this application somewhere\n              and hosting costs money. That is why we only require organizations\n              and companies to explore the options we have available to see what\n              suits them well.\n            </p>\n            <br />\n            <CardDeck>{tierCards}</CardDeck>\n          </div>\n        </Container>\n      </div>\n    );\n  }\n}\n\nexport default Pricing;\n"},"lineCount":null}},"error":null,"hash":"afbfdf785a3b7daf59bbe717704e136c","cacheData":{"env":{}}}